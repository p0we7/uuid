<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- array of downloads.-->
    <key>items</key>
    <array>
        <dict>
            <!-- an array of assets to download -->
            <key>assets</key>
            <array>
                <dict>
                    <!-- software-package: the ipa to install.-->
                    <key>kind</key>
                    <string>software-package</string>
                    <!-- required.the URL of the file to download.-->
                    <key>url</key>
                    <string>http://10.0.0.100:8000/%E5%BE%AE%E4%BF%A1_03%202.ipa</string>
                </dict>
                <!-- display-image: the icon to display during download .-->
                    <dict>
                        <key>kind</key>
                        <string>display-image</string>
                        <!-- optional.indicates if icon needs shine effect applied.-->
                        <key>needs-shine</key>
                        <true/>
                        <key>url</key>
                        <string>http://10.0.0.100:8000/AAAABbpiPTiGdf9lzOjVwh7tD852zp6WxEJH_Hxakwzg9UihBkai99oCBHrci8gpq9C8gESLMmUe42qRdqhSnWkiDW3QSy4Ngq3cHMFv.jpg</string>
                    </dict> 
            </array>
            <key>metadata</key>
            <dict>
                <!-- required -->
                <key>bundle-identifier</key>
                <string>com.tencent.xin</string>
                <!-- optional (software only) -->
                <key>bundle-version</key>
                <string>8.0.38</string>
                <!-- required.the download kind.-->
                <key>kind</key>
                <string>software</string>
                <!-- optional. displayed during download; typically company name -->
                <key>subtitle</key>
                           <string>Peter</string>
                <!-- required.the title to display during the download.-->
                <key>title</key>
                <string>微信</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
